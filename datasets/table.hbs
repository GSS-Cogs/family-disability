<h1>{{family}} Dataset Family</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Publisher</th>
      <th>Dataset</th>
    </tr>
  </thead>
  <tbody>
    {{#each pipelines}}
    <tr>
      <td>{{this.info.publisher}}</td>
      <td><a href="?{{this.directory}}">{{this.info.title}}</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
