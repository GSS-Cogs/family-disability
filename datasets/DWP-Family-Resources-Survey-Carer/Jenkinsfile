pipeline {
    agent {
        label 'master'
    }
    stages {
        stage('Transform') {
            agent {
                docker {
                    image 'cloudfluff/databaker'
                    reuseNode true
                }
            }
            steps {
                 def cmdArray = ["python", "-c", "print('hello')"]
                def cmd = cmdArray.execute()
                cmd.waitForOrKill(1000)
                println cmd.text
            }
        }
  
    }
    
}

