pipeline {
    agent {
        label 'master'
    }
    stages {
        stage('Transform') {
            agent {
                docker {
                    image 'cloudfluff/databaker'
                    reuseNode true
                }
            }
            steps {
                 sh 'jupyter-nbconvert --to python --stdout DWP-Family-Resources-Survey-Carer/main.ipynb | python'
            }
        }
  
    }
    
}

